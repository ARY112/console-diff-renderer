{
    "name": "graze/buffered-console",
    "description": "only write output to a console that has changed",
    "keywords": [
        "graze",
        "buffered-console"
    ],
    "homepage": "https://github.com/graze/buffered-console",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Bragg",
            "email": "harry.bragg@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^5.6 | ^7.0",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 | ^5.0",
        "squizlabs/php_codesniffer": "^2.5,!=2.8.1",
        "graze/standards": "^1.0",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Graze\\BufferedConsole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\BufferedConsole\\Test\\": "tests/src",
            "Graze\\BufferedConsole\\Test\\Unit\\": "tests/unit",
            "Graze\\BufferedConsole\\Test\\Integration\\": "tests/integration"
        }
    }
}
